<?xml version="1.0" encoding="UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:MauiApp2"
             xmlns:converters1="clr-namespace:MauiApp2.Converters"
             x:Class="MauiApp2.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
            
            <converters1:BoolToAvailabilityConverter x:Key="BoolToAvailabilityConverter" />
            <converters1:BoolToColorConverter x:Key="BoolToColorConverter" />
            <converters1:StringNotEmptyConverter x:Key="StringNotEmptyConverter" />
            <converters1:IsNotNullConverter x:Key="IsNotNullConverter" />
            <converters1:BoolInverterConverter x:Key="BoolInverterConverter" />
            
        </ResourceDictionary>
    </Application.Resources>
</Application>